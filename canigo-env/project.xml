		<sourceDirectory>src/main/java</sourceDirectory>

		<resources>
			<resource>
				<directory>src/main/resources</directory>
			</resource>
		</resources>

		<unitTestSourceDirectory>src/test/java</unitTestSourceDirectory>
		<unitTest>
			<includes>
				<include>**/*Test.java</include>
			</includes>
			
			<!--  Exclude the JWebUnit tests which require a J2EE webapp container (waiting for Jetty / Cactus / Cargo !)-->
			<excludes>
				<exclude>**/*WebTest.java</exclude>
				<exclude>**/*IntegrationTest.java</exclude>
			</excludes>

			<resources>
				<!-- Resource folder containing lo4j configuration, log4j.properties must be in the classpath -->
				<resource>
					<directory>src/test/resources</directory>
				</resource>
			</resources>

